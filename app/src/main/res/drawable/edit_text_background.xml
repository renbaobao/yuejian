<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#e1d9d9" />

            <corners android:radius="@dimen/d20" />
            <stroke
                android:width="0.1px"
                android:color="#c7c6c6" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/d10"
        android:left="@dimen/d10"
        android:right="@dimen/d10"
        android:top="@dimen/d10">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#e1d9d9" />

            <corners android:radius="@dimen/d20" />
            <stroke
                android:width="0.1px"
                android:color="#dcdbdb" />
        </shape>
    </item>
</layer-list>
